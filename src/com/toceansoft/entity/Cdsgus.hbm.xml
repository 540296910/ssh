<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.toceansoft.entity"  >
	<class name="Cdsgus" table="cdsgus" lazy="false">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="Name" length="255"></property>
		<property name="ctfid" column="CtfId" length="255" ></property>
		<property name="address" column="Address" length="255" ></property>
		<property name="version" column="Version" length="255" ></property>
	</class>	
</hibernate-mapping>
